    "prettier": "2.4.1",
    "pretty-quick": "3.1.2",
    "single-spa-react": "3.2.0",
    "ts-config-single-spa": "2.0.1",
    "typescript": "4.5.2",
    "webpack": "5.64.3",
    "webpack-cli": "4.9.1",
    "webpack-config-single-spa-react": "4.0.2",
    "webpack-config-single-spa-react-ts": "4.0.2",
    "webpack-config-single-spa-ts": "2.2.4",
    "webpack-dev-server": "4.5.0",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@types/jest": "^26.0.16",
    "@types/react": "16.14.21",
    "@types/react-dom": "16.9.14",
    "@types/systemjs": "6.1.1",
